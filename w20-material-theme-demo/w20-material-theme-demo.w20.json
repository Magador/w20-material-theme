{
  "id": "w20-material-theme-demo",
  "name": "W20 material Theme Demo",
  "home": "/home",

  "modules": {
    "home": {
      "path": "{w20-material-theme-demo}/modules/home",
      "autoload": true
    },
    "places": {
      "path": "{w20-material-theme-demo}/modules/places",
      "autoload": true
    },
    "issues": {
      "path": "{w20-material-theme-demo}/modules/issues",
      "autoload": true
    },
    "contacts": {
      "path": "{w20-material-theme-demo}/modules/contacts",
      "autoload": true
    },
    "style": {
      "path": "[css]!{w20-material-theme-demo}/style/style.css",
      "autoload": true
    },
    "google-maps": {
      "path": "https://maps.googleapis.com/maps/api/js?key=AIzaSyDuFxOss-BwVn6MGAu-5kJypx0Fv25NhZg&libraries=places",
      "autoload": true
    }
  },
  "i18n": {
    "en": ["{w20-material-theme-demo}/i18n/messages-en.json"],
    "fr": ["{w20-material-theme-demo}/i18n/messages-fr.json"]
  },
  "routes": {
    "/home": {
      "templateUrl": "{w20-material-theme-demo}/views/home.html",
      "controller": "HomeController",
      "hidden": false,
      "icon": "home",
      "search": {
        "disabled": true
      }
    },
    "/places": {
      "templateUrl": "{w20-material-theme-demo}/views/places.html",
      "controller": "PlaceController",
      "hidden": false,
      "icon": "map",
      "search": {
        "placeholder": "application.view.w20-material-theme-demo.places.search.placeholder"
      }
    },
    "/issues": {
      "templateUrl": "{w20-material-theme-demo}/views/issues.html",
      "controller": "IssueController",
      "hidden": false,
      "icon": "error_outline",
      "search": {
        "disabled": true
      }
    },
    "/contacts": {
      "templateUrl": "{w20-material-theme-demo}/views/contacts.html",
      "controller": "ContactController",
      "hidden": false,
      "icon": "people",
      "search": {
        "placeholder": "application.view.w20-material-theme-demo.contacts.search.placeholder"
      }
    }
  },
  "requireConfig": {
    "paths": {
      "{angular-messages}": "bower_components/angular-messages"
    },
    "shim": {
      "{angular-messages}/angular-messages": ["{angular}/angular"]
    }
  }
}